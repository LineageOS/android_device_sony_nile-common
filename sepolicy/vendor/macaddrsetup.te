type macaddrsetup, domain;
type macaddrsetup_exec, exec_type, vendor_file_type, file_type;

init_daemon_domain(macaddrsetup)

# Allow macaddrsetup to access tad
unix_socket_connect(macaddrsetup, tad, tad)

# Allow macaddrsetup to read, write and create files in bluetooth_data_file
allow macaddrsetup bluetooth_data_file:dir rw_dir_perms;
allow macaddrsetup bluetooth_data_file:file create_file_perms;

# Allow macaddrsetup to read, write and create files in wifi_data_file
allow macaddrsetup wifi_data_file:dir rw_dir_perms;
allow macaddrsetup wifi_data_file:file create_file_perms;
